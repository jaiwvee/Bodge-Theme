	<!-- Functions -->
	<key>Functions</key>
	
	<dict>

		<key>rotateArrow</key>
		<array>
		
			<string>get.views.wrapper.contentOffsetY = 0 ? views.arrow.rotation = 180 animated:0.15 : nothing</string>
			<string>get.views.wrapper.contentOffsetY = 392 ? views.arrow.rotation = 0 animated:0.15 : nothing</string>
			
		</array>

		<key>preventOverscroll</key>
		<array>

			<string>get.views.wrapper.contentOffsetY lt 0 ? views.wrapper.contentOffsetY = 0 : nothing</string>
			<string>get.views.wrapper.contentOffsetY gt 392 ? views.wrapper.contentOffsetY = 392 : nothing</string>

		</array>
		
	</dict>
	<!-- // Functions -->
	
</dict>
</plist>